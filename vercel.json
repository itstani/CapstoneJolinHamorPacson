{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "access-control-allow-origin": "*"
      },
      "dest": "/images/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/debug-images",
      "dest": "/server.js"
    },
    {
      "src": "/Webpages/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}