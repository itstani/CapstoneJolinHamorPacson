{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Webpages/**",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    },
    {
      "src": "CSS/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/CSS/(.*)",
      "dest": "/CSS/$1"
    },
    {
      "src": "/(.*)\\.html",
      "dest": "/Webpages/$1.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/images/favicon.ico"
    },
    {
      "src": "/(login|register|profile)",
      "dest": "/Webpages/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}