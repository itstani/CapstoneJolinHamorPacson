{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    },
    {
      "src": "CSS/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/images/$1"
    },
    {
      "src": "/CSS/(.*)",
      "dest": "/CSS/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/images/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}

