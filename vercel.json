{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "access-control-allow-origin": "*"
      },
      "dest": "/index.js"
    },
    {
      "src": "/CSS/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/Webpages/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}

