{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Webpages/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "CSS/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/CSS/(.*)",
      "dest": "/CSS/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/Webpages/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/Webpages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
