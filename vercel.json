{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "Webpages/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "CSS/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "images/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/CSS/(.*)",
        "dest": "/CSS/$1"
      },
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/login",
        "dest": "/Webpages/login.html"
      },
      {
        "src": "/(.*)\\.html",
        "dest": "/Webpages/$1.html"
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }